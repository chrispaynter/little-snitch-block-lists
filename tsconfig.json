{
  "compilerOptions": {
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "lib": ["es6"],
      "moduleResolution": "node",
      "noEmit": true,
      "strict": true,
      "target": "esnext",
      "skipLibCheck": true,
      "baseUrl": "./",
      "resolveJsonModule": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true
  },
  "exclude": [
      "node_modules",
  ]
}
